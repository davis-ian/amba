indow show-arrows continue v-model=\"window\">\n        <v-window-item v-for=\"(item, index) in featured_posts\" :key=\"index\">\n          <div>\n            <img\n              v-if=\"item.pic\"\n              class=\"pt-5 pb-5\"\n              :src=\"`https://59esd0ee.directus.app/assets/${item.pic}`\"\n              alt=\"featured image\"\n            />\n            <img\n              v-else\n              class=\"pt-5 pb-5\"\n              style=\"max-height: 400px\"\n              src=\"https://www.thetipsymacaron.com/wp-content/uploads/cotton-candy-macarons.jpg\n            \"\n              alt=\"\"\n            />\n\n            <h4>{{ item.title }}</h4>\n            <p>{{ item.body }}</p>\n            <v-btn dark @click=\"getDetails(item.id)\">details</v-btn>\n          </div>\n        </v-window-item>\n      </v-window>\n    </v-card>\n    <!-- <v-btn @click=\"toggleModal\">toggle</v-btn> -->\n\n    <v-dialog v-model=\"dialog\">\n      <v-card>\n        <v-card-title>Modal</v-card-title>\n        <v-card-text>this is text</v-card-text>\n        <v-data-table\n          :items=\"featured_posts\"\n          :headers=\"postHeaders\"\n        ></v-data-table>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      message: \"my message\",\n      dialog: false,\n      window: 0,\n      featured_posts: [],\n      postHeaders: [{ text: \"Title\", value: \"title\" }],\n    };\n  },\n  mounted() {\n    this.getPosts();\n  },\n  methods: {\n    toggleModal() {\n      this.dialog = true;\n    },\n    getPosts() {\n      axios\n        .get(\"https://59esd0ee.directus.app/items/posts?filter[featured]=true\")\n        .then((response) => {\n          console.log(\"success\", response.data);\n          this.featured_posts = response.data.data;\n        })\n        .catch((error) => {\n          console.log(\"error\", error);\n        });\n    },\n    getDetails(id) {\n      this.$router.push({ path: `/details/${id}` });\n    },\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module"}