\n            <h4>{{ item.title }}</h4>\n            <p>{{ item.body }}</p>\n            <v-btn dark @click=\"getDetails(item.id)\">details</v-btn>\n          </div>\n        </v-window-item>\n      </v-window>\n    </v-card>\n    <v-btn @click=\"toggleModal\">toggle</v-btn>\n\n    <v-dialog v-model=\"dialog\">\n      <v-card>\n        <v-card-title>Modal</v-card-title>\n        <v-card-text>this is text</v-card-text>\n        <v-data-table :items=\"posts\" :headers=\"postHeaders\"></v-data-table>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      message: \"my message\",\n      dialog: false,\n      window: 0,\n      posts: [],\n      postHeaders: [{ text: \"Title\", value: \"title\" }],\n    };\n  },\n  mounted() {\n    this.getPosts();\n  },\n  methods: {\n    toggleModal() {\n      this.dialog = true;\n    },\n    getPosts() {\n      axios\n        .get(\"https://59esd0ee.directus.app/items/posts\")\n        .then((response) => {\n          console.log(\"success\", response.data);\n          this.posts = response.data.data;\n        })\n        .catch((error) => {\n          console.log(\"error\", error);\n        });\n    },\n    getDetails() {\n      this.$router.push({ name: \"details\" });\n    },\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module"}