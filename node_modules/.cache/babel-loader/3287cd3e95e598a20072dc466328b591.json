{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      message: \"HI\",\n      recipes: []\n    };\n  },\n\n  mounted() {\n    this.getRecipes();\n  },\n\n  methods: {\n    recipeDetails(item) {\n      this.$router.push(`recipe/${item.id}`);\n    },\n\n    getRecipes() {\n      axios.get(\"https://59esd0ee.directus.app/items/recipes\").then(response => {\n        console.log(\"success\", response.data.data);\n        this.recipes = response.data.data;\n      }).catch(error => {\n        console.log(\"error\", error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8BA;AACA;EACAA;IACA;MACAC,aADA;MAEAC;IAFA;EAIA,CANA;;EAOAC;IACA;EACA,CATA;;EAUAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACAC,MACAC,GADA,CACA,6CADA,EAEAC,IAFA,CAEAC;QACAC;QACA;MACA,CALA,EAMAC,KANA,CAMAC;QACAF;MACA,CARA;IASA;;EAdA;AAVA","names":["data","message","recipes","mounted","methods","recipeDetails","getRecipes","axios","get","then","response","console","catch","error"],"sourceRoot":"src/views","sources":["RecipeListView.vue"],"sourcesContent":["<template>\n  <div id=\"rl-main\">\n    <h1 class=\"text-center abril\">recipe list</h1>\n    <div id=\"r-wrapper\" class=\"ma-3\">\n      <div\n        @click=\"recipeDetails(item)\"\n        v-for=\"(item, index) in recipes\"\n        :key=\"index\"\n        class=\"elevation-3 recipe\"\n      >\n        <img\n          v-if=\"item.pic\"\n          :src=\"`https://59esd0ee.directus.app/assets/${item.pic}`\"\n          alt=\"featured image\"\n        />\n        <img\n          v-else\n          src=\"https://www.thetipsymacaron.com/wp-content/uploads/cotton-candy-macarons.jpg\n            \"\n          alt=\"featured image\"\n        />\n        <span class=\"text-center recipe-title\">\n          <strong>{{ item.name }}</strong>\n        </span>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      message: \"HI\",\n      recipes: [],\n    };\n  },\n  mounted() {\n    this.getRecipes();\n  },\n  methods: {\n    recipeDetails(item) {\n      this.$router.push(`recipe/${item.id}`);\n    },\n    getRecipes() {\n      axios\n        .get(\"https://59esd0ee.directus.app/items/recipes\")\n        .then((response) => {\n          console.log(\"success\", response.data.data);\n          this.recipes = response.data.data;\n        })\n        .catch((error) => {\n          console.log(\"error\", error);\n        });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.recipe {\n  background-color: white;\n  border-radius: 10px;\n  transition: 0.3s;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  margin: 30px 10px;\n  /* border: 2px solid red; */\n}\n.recipe img {\n  border-radius: 10px 10px 0 0;\n}\n\n.recipe:hover {\n  cursor: pointer;\n  /* background-color: rgba(211, 211, 211, 0.761); */\n  transition: 0.3s;\n}\n.recipe-title {\n  padding: 10px;\n}\n\n#rl-main {\n  background-color: #f6e4d1;\n  /* border: 2px solid red; */\n  min-height: calc(100vh - 56px);\n  padding: 20px 0;\n}\n\n@media (min-width: 795px) {\n  .recipe {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    transition: 0.3s;\n    position: relative;\n    bottom: 0;\n  }\n  .recipe:hover {\n    bottom: 20px;\n    transition: 0.3s;\n  }\n  .recipe img {\n    /* border: 2px solid red; */\n    max-height: 400px;\n  }\n\n  #r-wrapper {\n    display: flex;\n    justify-content: space-evenly;\n    /* border: 2px solid orange; */\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}